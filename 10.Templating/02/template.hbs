<article>
    {{#each contacts}}
        {{#if name}}
            <div class="title" s>
                {{name}}
                <button>&#8505;</button>
            </div>
        {{else}}
            <div>No Name</div>
        {{/if}}

        <div class="info">
            <p>
                &phone;{{phone}}
            </p>
            <p>
                &#9993;{{email}}
            </p>
        </div>

    {{/each}}
</article>